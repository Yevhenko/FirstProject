  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/index.ts",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "local:dev:typeorm": "npm run typeorm:dev",
    "typeorm:dev": "ts-node ./node_modules/.bin/typeorm --config ormconfig.ts",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm -- migration:generate --config src/config/ormconfig.ts --connection  --name ",
    "migration:run": "npm run typeorm -- migration:run",
    "seeder:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seeder:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
